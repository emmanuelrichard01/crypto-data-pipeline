{
  "dashboard": {
    "id": null,
    "title": "AI Analytics",
    "tags": ["ai", "predictions", "analytics"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "AI Confidence Index (Avg)",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT AVG(ai_confidence_index) as avg_confidence FROM marts.mart_ai_predictions WHERE timeframe = '1D'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 2,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 0.6 },
                { "color": "green", "value": 0.8 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Prediction Accuracy (MAPE)",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT AVG(mape) as avg_mape FROM marts.mart_ai_predictions WHERE timeframe = '1D'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 5 },
                { "color": "red", "value": 10 }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "High Confidence Predictions",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT COUNT(*) as high_confidence_count FROM marts.mart_ai_predictions WHERE prediction_quality_category = 'High' AND timeframe = '1D'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [{ "color": "blue", "value": null }]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Prediction Deviation Alerts",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT COUNT(*) as alert_count FROM alerts WHERE alert_type = 'Prediction Deviation' AND created_at >= NOW() - INTERVAL '1 hour'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "graph",
        "title": "AI Confidence Index Over Time",
        "gridPos": { "x": 0, "y": 4, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT prediction_date as time, symbol, ai_confidence_index FROM marts.mart_ai_predictions WHERE timeframe = '1D' AND prediction_date >= CURRENT_DATE - INTERVAL '7 days' ORDER BY prediction_date",
            "format": "time_series"
          }
        ],
        "options": {
          "legend": {
            "show": true,
            "position": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 2
          }
        }
      },
      {
        "id": 6,
        "type": "graph",
        "title": "Prediction Accuracy (MAPE) Over Time",
        "gridPos": { "x": 0, "y": 12, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT prediction_date as time, symbol, mape FROM marts.mart_ai_predictions WHERE timeframe = '1D' AND prediction_date >= CURRENT_DATE - INTERVAL '7 days' ORDER BY prediction_date",
            "format": "time_series"
          }
        ],
        "options": {
          "legend": {
            "show": true,
            "position": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 7,
        "type": "table",
        "title": "Latest AI Predictions",
        "gridPos": { "x": 0, "y": 20, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT symbol, prediction_date, predicted_price, confidence_score, ai_confidence_index, prediction_quality_category, mape FROM marts.mart_ai_predictions WHERE timeframe = '1D' ORDER BY prediction_date DESC LIMIT 50",
            "format": "table"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [{ "id": "prediction_date", "desc": true }]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto"
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "confidence_score" },
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "color": "red", "value": null },
                      { "color": "orange", "value": 0.6 },
                      { "color": "green", "value": 0.8 }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "ai_confidence_index" },
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "color": "red", "value": null },
                      { "color": "orange", "value": 0.6 },
                      { "color": "green", "value": 0.8 }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "mape" },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "color": "green", "value": null },
                      { "color": "orange", "value": 5 },
                      { "color": "red", "value": 10 }
                    ]
                  }
                }
              ]
            }
          ]
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}
