{
  "dashboard": {
    "id": null,
    "title": "Pipeline Monitoring",
    "tags": ["pipeline", "monitoring", "etl"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Pipeline Status",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT CASE WHEN COUNT(*) > 0 THEN 'Running' ELSE 'Stopped' END as status FROM marts.mart_pipeline_monitoring WHERE run_date = CURRENT_DATE",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "green", "value": 1 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Success Rate (24h)",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT AVG(success_rate_pct) as success_rate FROM marts.mart_pipeline_monitoring WHERE run_date >= CURRENT_DATE - INTERVAL '1 day'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 80 },
                { "color": "green", "value": 95 }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Avg Duration (24h)",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT AVG(avg_duration_seconds) as avg_duration FROM marts.mart_pipeline_monitoring WHERE run_date >= CURRENT_DATE - INTERVAL '1 day'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 60 },
                { "color": "red", "value": 120 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Total Records Processed (24h)",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT SUM(total_records_processed) as total_records FROM marts.mart_pipeline_monitoring WHERE run_date >= CURRENT_DATE - INTERVAL '1 day'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [{ "color": "blue", "value": null }]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "graph",
        "title": "Pipeline Performance Over Time",
        "gridPos": { "x": 0, "y": 4, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT run_date as time, stage, avg_duration_seconds FROM marts.mart_pipeline_monitoring WHERE run_date >= CURRENT_DATE - INTERVAL '7 days' ORDER BY run_date",
            "format": "time_series"
          }
        ],
        "options": {
          "legend": {
            "show": true,
            "position": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2
          }
        }
      },
      {
        "id": 6,
        "type": "table",
        "title": "Recent Pipeline Runs",
        "gridPos": { "x": 0, "y": 12, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT run_date, stage, total_runs, successful_runs, success_rate_pct, avg_duration_seconds, latest_run_status FROM marts.mart_pipeline_monitoring WHERE run_date >= CURRENT_DATE - INTERVAL '7 days' ORDER BY run_date DESC, stage",
            "format": "table"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [{ "id": "run_date", "desc": true }]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto"
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "success_rate_pct" },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "color": "red", "value": null },
                      { "color": "orange", "value": 80 },
                      { "color": "green", "value": 95 }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "avg_duration_seconds" },
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                }
              ]
            }
          ]
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}
