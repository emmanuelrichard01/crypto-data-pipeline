{
  "dashboard": {
    "id": null,
    "title": "Liquidity Metrics",
    "tags": ["liquidity", "orderbook", "market depth"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Average Liquidity Index",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT AVG(avg_liquidity_index) as avg_liquidity FROM marts.mart_liquidity_metrics",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 2,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 0.4 },
                { "color": "green", "value": 0.7 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Healthy Liquidity Pairs",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT COUNT(*) as healthy_count FROM marts.mart_liquidity_metrics WHERE liquidity_health = 'Healthy'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [{ "color": "green", "value": null }]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Average Spread %",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT AVG(avg_spread_percentage) as avg_spread FROM marts.mart_liquidity_metrics",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 0.1 },
                { "color": "red", "value": 0.3 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Low Liquidity Alerts",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "rawSql": "SELECT COUNT(*) as alert_count FROM alerts WHERE alert_type = 'Liquidity Drop' AND created_at >= NOW() - INTERVAL '1 hour'",
            "format": "table"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "graph",
        "title": "Liquidity Index Over Time",
        "gridPos": { "x": 0, "y": 4, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT timestamp as time, symbol, avg_liquidity_index FROM marts.mart_liquidity_metrics WHERE timestamp >= NOW() - INTERVAL '7 days' ORDER BY timestamp",
            "format": "time_series"
          }
        ],
        "options": {
          "legend": {
            "show": true,
            "position": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 2
          }
        }
      },
      {
        "id": 6,
        "type": "graph",
        "title": "Spread Percentage Over Time",
        "gridPos": { "x": 0, "y": 12, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT timestamp as time, symbol, avg_spread_percentage FROM marts.mart_liquidity_metrics WHERE timestamp >= NOW() - INTERVAL '7 days' ORDER BY timestamp",
            "format": "time_series"
          }
        ],
        "options": {
          "legend": {
            "show": true,
            "position": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 7,
        "type": "table",
        "title": "Latest Liquidity Metrics",
        "gridPos": { "x": 0, "y": 20, "w": 24, "h": 8 },
        "targets": [
          {
            "rawSql": "SELECT symbol, timestamp, avg_bid_price, avg_ask_price, avg_spread_percentage, avg_liquidity_index, liquidity_health, median_bid_depth, median_ask_depth FROM marts.mart_liquidity_metrics ORDER BY timestamp DESC LIMIT 50",
            "format": "table"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [{ "id": "timestamp", "desc": true }]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto"
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "avg_liquidity_index" },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "color": "red", "value": null },
                      { "color": "orange", "value": 0.4 },
                      { "color": "green", "value": 0.7 }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "avg_spread_percentage" },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "color": "green", "value": null },
                      { "color": "orange", "value": 0.1 },
                      { "color": "red", "value": 0.3 }
                    ]
                  }
                }
              ]
            }
          ]
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}
